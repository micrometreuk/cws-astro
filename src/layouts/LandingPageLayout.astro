    ></script>
    <script src="./vendor/swiper/swiper-bundle.min.js" is:inline></script>
    <script src="./vendor/swiper/swiper-bundle.js.map" is:inline></script>
    <script src="./vendor/gsap/gsap.min.js" is:inline></script>

    <script src="/js/script.js" async></script>
    <script src="/js/main.js" is:inline></script>
  </body>
</html>
